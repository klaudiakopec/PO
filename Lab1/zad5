package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a;


        System.out.println("Podaj wartosc a ");
        a = in.nextInt();
        System.out.println("Liczba a = "+a);

        int pot= a*a*a;
        System.out.println("Liczba podniesiona do szczescianu = "+pot);

    }
}
